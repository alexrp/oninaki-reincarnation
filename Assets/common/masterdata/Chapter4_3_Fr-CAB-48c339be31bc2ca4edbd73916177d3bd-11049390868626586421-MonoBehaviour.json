                {
                  "0 Param data": {
                    "1 string m_id": "EVTXT_431000_10",
                    "0 int m_subId": 400,
                    "1 string m_charaId": "CN_0010",
                    "1 string m_voiceId": "",
                    "1 string m_text": "Ce royaume appartient à son peuple ! Nous ne permettrons pas qu'un imbécile nous le prenne !"
