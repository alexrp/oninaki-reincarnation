                    "0 Param m_list": 
                    {
                        "0 Array Array": [
                            {"0 Param data": 
                            {
                                "1 string m_id": "CP_0000_0",
                                "1 string m_description": "冷静沈着。他者との関わりをあまり好まず、\n逝ク人守リとしての任務を冷然とこなす。"
                            }},
                            {"0 Param data": 
                            {
                                "1 string m_id": "CP_0000_1",
                                "1 string m_description": "リンネと行動を共にし、考え方が\n少しずつ変化しているが本人は無自覚。"
                            }},
                            {"0 Param data": 
                            {
                                "1 string m_id": "CP_0000_2",
                                "1 string m_description": "魂の片割れである黒夜叉を救う事が、\n自分に課せられた運命と感じる。"
                            }},
                            {"0 Param data": 
                            {
                                "1 string m_id": "CP_1000_0",
                                "1 string m_description": "？？？"
                            }},
                            {"0 Param data": 
                            {
                                "1 string m_id": "CP_1000_1",
                                "1 string m_description": "今と向き合うために何をなすべきか、\nその答えを求めて黒夜叉の過去を辿る。"
                            }},
                            {"0 Param data": 
                            {
                                "1 string m_id": "CP_2000_0",
                                "1 string m_description": "忌み子として殺される運命から逃げ延びた。しかし、その目にかつての輝きはない。"
                            }},
                            {"0 Param data": 
                            {
                                "1 string m_id": "CP_1000_2",
                                "1 string m_description": "運命を呪い、黒夜叉に成り果てた。\n邦の理にすがるだけの命を蔑む。"
                            }},
                            {"0 Param data": 
                            {
                                "1 string m_id": "CP_1000_3",
                                "1 string m_description": "命の粛清を続け、処刑人と呼ばれる。\n人々の恐れの対象になっている。"
                            }},
                            {"0 Param data": 
                            {
                                "1 string m_id": "CP_1000_4",
                                "1 string m_description": "ソウジュの身体にカガチの魂が宿った姿。\n過去を辿り、果たすべき目的を見つけた。"
                            }},
                            {"0 Param data": 
                            {
                                "1 string m_id": "CK_0001_0",
                                "1 string m_description": "敵を圧倒する流麗な剣技の持ち主。\nカガチの生き方に興味を持っている。"
                            }},
                            {"0 Param data": 
                            {
                                "1 string m_id": "CK_0001_1",
                                "1 string m_description": "愛に生き、愛を失った。\n強く純粋な気持ちが彼女を鬼ビ人にした。"
                            }},
                            {"0 Param data": 
                            {
                                "1 string m_id": "CK_0002_0",
                                "1 string m_description": "騎士道精神を持ち実直な性格。\n熟達した槍さばきで敵を圧倒する。"
                            }},
                            {"0 Param data": 
                            {
                                "1 string m_id": "CK_0002_1",
                                "1 string m_description": "救いたかった命を救えなかった後悔と\n己の無力さに苛まれ鬼ビ人に成り果てる。"
                            }},
                            {"0 Param data": 
                            {
                                "1 string m_id": "CK_0003_0",
                                "1 string m_description": "巨大な斧を巧みに操る少年。\n感情的で近寄りがたいが仲間想いの一面も。"
                            }},
                            {"0 Param data": 
                            {
                                "1 string m_id": "CK_0003_1",
                                "1 string m_description": "遥か昔に存在した生命の一族。\n深い憎悪と絶望が彼を鬼ビ人に変えた。"
                            }},
                            {"0 Param data": 
                            {
                                "1 string m_id": "CK_0004_0",
                                "1 string m_description": "長大な鎌を振るう少女。\n無表情で無感情。他人への興味が薄い。"
                            }},
                            {"0 Param data": 
                            {
                                "1 string m_id": "CK_0004_1",
                                "1 string m_description": "邦の理に疑念を持ち自ら命を絶つが、\nそれでも安らぎは得られず鬼ビ人となった。"
                            }},
                            {"0 Param data": 
                            {
                                "1 string m_id": "CK_0005_0",
                                "1 string m_description": "双剣を巧みに使う赤髪の青年。\n普段は穏やかだが戦いでは攻撃的になる。"
                            }},
                            {"0 Param data": 
                            {
                                "1 string m_id": "CK_0005_1",
                                "1 string m_description": "盗賊に育てられた過去を持つ。\n孤独への嘆きが彼を鬼ビ人にした。"
                            }},
                            {"0 Param data": 
                            {
                                "1 string m_id": "CK_0006_0",
                                "1 string m_description": "弩と銃を手足のように操る赤髪の麗人。\nその振る舞いは上品にして優雅。\n"
                            }},
                            {"0 Param data": 
                            {
                                "1 string m_id": "CK_0006_1",
                                "1 string m_description": "弟を失い、自分自身も見失う。\n偽り続けた人生への未練と後悔で\n鬼ビ人となる。\n"
                            }},
                            {"0 Param data": 
                            {
                                "1 string m_id": "CK_0007_0",
                                "1 string m_description": "盾と大砲を組み合わせた武器を使用する。\n口数は少なく物静か。"
                            }},
                            {"0 Param data": 
                            {
                                "1 string m_id": "CK_0007_1",
                                "1 string m_description": "戦乱の時代を生きた傭兵。\n何も守れなかった自責の念に縛られ\n鬼ビ人に成り果てる。"
                            }},
                            {"0 Param data": 
                            {
                                "1 string m_id": "CK_0008_0",
                                "1 string m_description": "強力な力場を発生させる手袋を持つ。\n好奇心が強く自由を愛する女性。"
                            }},
                            {"0 Param data": 
                            {
                                "1 string m_id": "CK_0008_1",
                                "1 string m_description": "人知を超えた頭脳の持ち主。\n影ナシの魂を取り込み永遠の命を求めた。"
                            }},
                            {"0 Param data": 
                            {
                                "1 string m_id": "CK_0009_0",
                                "1 string m_description": "鎖でできた鞭を生物のように自在に操る。\n饒舌で陽気に見えるが、その本性は冷酷。"
                            }},
                            {"0 Param data": 
                            {
                                "1 string m_id": "CK_0009_1",
                                "1 string m_description": "人間の行動心理に興味を持つ。\n死の間際でさえその欲望は膨らみ、\n終末の観察を渇望した。"
                            }},
                            {"0 Param data": 
                            {
                                "1 string m_id": "CK_0011_0",
                                "1 string m_description": "義理を重んじる孤高の狼。\n鋭い爪はあらゆるものを切り裂く。"
                            }},
                            {"0 Param data": 
                            {
                                "1 string m_id": "CK_0011_1",
                                "1 string m_description": "好敵手との戦いが心に変化をもたらす。\n最後に望んだものは守る者の存在だった。"
                            }},
                            {"0 Param data": 
                            {
                                "1 string m_id": "CK_0012_0",
                                "1 string m_description": "膨大な知識から武器を具現化する。\n機械的な受け答えしかしない。"
                            }},
                            {"0 Param data": 
                            {
                                "1 string m_id": "CK_0012_1",
                                "1 string m_description": "別次元から訪れ、世界を記録し続けている。厳密には鬼ビ人ではない特殊な存在。"
                            }}
                        ]
                    }
                }}
